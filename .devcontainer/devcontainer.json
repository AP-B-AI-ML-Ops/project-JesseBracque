{
    "name": "MLOps-project",
    "dockerComposeFile": ["../../docker-compose.yml"], // locate the docker compose file
    "service": "train-dev", // locate the service used in the docker compose file
    "shutdownAction": "none",
    "workspaceFolder": "/workspace/train", // locate the root folder for this service
    "overrideCommand": false,
    "runServices": ["train-dev"],
    "customizations": { // define which extensions we will need for our dev env
        "vscode": {
            "extensions": [
                "ms-toolsai.jupyter"
            ]
        }
    }
}



